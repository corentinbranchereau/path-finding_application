<VBox prefHeight="749.0" prefWidth="1413.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.hexaone.view.FenetreControleurFXML">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="Fichier">
               <items>
                  <MenuItem fx:id="chargerCarteItem" mnemonicParsing="false" text="Charger une carte" />
                  <MenuItem fx:id="chargerRequetesItem" mnemonicParsing="false" text="Charger demande de livraison" />
                  <MenuItem fx:id="quitterItem" mnemonicParsing="false" text="Quitter" />
               </items>
               </Menu>
               <Menu mnemonicParsing="false" text="Edition">
               <items>
                  <MenuItem fx:id="undoItem" mnemonicParsing="false" text="Undo" />
                  <MenuItem fx:id="redoItem" mnemonicParsing="false" text="Redo" />

               </items>
            </Menu>
         </menus>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </MenuBar>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <AnchorPane fx:id="anchorPaneGraphique" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="900.0">
               <children>
                  <Rectangle fx:id="bordureCarte" arcHeight="5.0" arcWidth="5.0" fill="#ffffff00" height="700.0" stroke="BLACK" strokeType="INSIDE" width="900.0" />
                  <Pane fx:id="paneDessin" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="900.0" />
               </children>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </HBox.margin>
            </AnchorPane>
            <VBox alignment="TOP_CENTER" prefHeight="710.0" prefWidth="488.0" style="-fx-background-color: white;">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="boutonLancer" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Lancer le calcul" visible="false">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="boutonNouvelleRequete" alignment="CENTER" contentDisplay="CENTER" disable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Ajouter demande de livraison">
                           <HBox.margin>
                              <Insets left="20.0" right="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="boutonModifierPlanning" alignment="CENTER" contentDisplay="CENTER" disable="true" layoutX="150.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Modifier l'ordre">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <GridPane alignment="TOP_CENTER" hgap="10" prefHeight="48.0" prefWidth="488.0" vgap="10">
                     <padding>
                        <Insets bottom="10" left="0" right="0" top="0" />
                     </padding>
                     <columnConstraints>
                        <ColumnConstraints halignment="RIGHT" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
                        <ColumnConstraints />
                     </columnConstraints>
                     <children>
                        <!-- Add PickupDuration Label -->
                        
                        <Label fx:id="pickupDurationLabel" text="Durée Collecte" visible="false" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                        <!-- Add PickupDuration Text Field -->
                        <TextField fx:id="pickupDurationField" disable="true" prefHeight="40" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />

                        <!-- Add deliveryDuration Label -->
                        <Label fx:id="deliveryDurationLabel" text="Durée Livraison" visible="false" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                        <!-- Add deliveryDuration Field -->
                        <TextField fx:id="deliveryDurationField" disable="true" prefHeight="40" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        
                        <Label fx:id="durationLabel" text="Nouvelle durée" visible="false" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                        <!-- Add PickupDuration Text Field -->
                        <TextField fx:id="durationField" disable="true" prefHeight="40" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     </children>
                     <rowConstraints>
                        <RowConstraints />
                        <RowConstraints />
                        <RowConstraints />
                     </rowConstraints>
                  </GridPane>
                  <HBox fx:id="boxBoutonsValiderAnnuler" alignment="TOP_CENTER" prefHeight="17.0" prefWidth="488.0" visible="false">
                     <children>
                        <Button fx:id="boutonValider" alignment="CENTER" contentDisplay="CENTER" disable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Valider">
                           <HBox.margin>
                              <Insets bottom="7.0" />
                              <Insets left="20.0" />
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="boutonValiderModificationDemande" alignment="CENTER" contentDisplay="CENTER" disable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Valider">
                           <HBox.margin>
                              <Insets bottom="7.0" />
                              <Insets left="20.0" />
                              <Insets right="20.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="boutonAnnuler" alignment="CENTER" contentDisplay="CENTER" disable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Annuler">
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="10" left="0" right="0" top="0" />
                     </padding>
                  </HBox>
                  <TextFlow fx:id="depotTextInformation" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <Text lineSpacing="20.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Pour charger une Carte ou des Requêtes, rendez-vous dans &quot;Fichier&quot;, en haut à gauche de l'application." />
                     </children>
                  </TextFlow>
                  <ScrollPane fx:id="textScrollPane" prefHeight="630.0" prefWidth="488.0" style="-fx-background-color: white;">
                     <content>
                        <TextFlow fx:id="zoneTexte" prefHeight="493.0" prefWidth="523.0" style="-fx-padding: 10; -fx-background-color: white;" />
                     </content>
                  </ScrollPane>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
