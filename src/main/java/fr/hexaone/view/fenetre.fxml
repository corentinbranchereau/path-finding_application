<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<VBox prefHeight="749.0" prefWidth="1350.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.hexaone.view.FenetreControleurFXML">
   <children>
      <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="Fichier">
               <items>
                  <MenuItem fx:id="chargerCarteItem" mnemonicParsing="false" text="Charger une carte" />
                  <MenuItem fx:id="chargerRequetesItem" mnemonicParsing="false" text="Charger demande de livraison" />
                  <MenuItem fx:id="quitterItem" mnemonicParsing="false" text="Quitter" />
               </items>
            </Menu>
         </menus>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </MenuBar>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <AnchorPane fx:id="anchorPaneGraphique" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="900.0">
               <children>
                  <Rectangle fx:id="bordureCarte" arcHeight="5.0" arcWidth="5.0" fill="#ffffff00" height="700.0" stroke="BLACK" strokeType="INSIDE" width="900.0" />
                  <Pane fx:id="paneDessin" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="900.0" />
               </children>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </HBox.margin>
            </AnchorPane>
            <VBox alignment="TOP_CENTER" prefHeight="710.0" prefWidth="416.0">
               <children>
                  <Button fx:id="boutonLancer" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Lancer le calcul">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="boutonNouvelleRequete" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Nouvelle demande de livraison">
                     <VBox.margin>
                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Button>
                  <HBox alignment="TOP_CENTER" prefWidth="320.0" prefHeight="100">
                     <Button fx:id="boutonValider" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Valider" disable="true">
                        <HBox.margin>
                           <Insets bottom="7.0" />
                           <Insets left="20.0"/>
                           <Insets right="20.0"/>
                        </HBox.margin>
                     </Button>
                     <Button fx:id="boutonAnnuler" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Annuler" disable="true">
                        <VBox.margin>
                           <Insets bottom="10.0" />
                        </VBox.margin>
                     </Button>
                  </HBox>
                  <ScrollPane prefHeight="700.0" prefWidth="320.0">
                  <ScrollPane prefHeight="664.0" prefWidth="320.0">
                     <content>
                        <TextFlow fx:id="zoneTexte" prefHeight="664.0" prefWidth="393.0" style="-fx-padding: 20;">
                           <children>
                              <Text lineSpacing="20.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Pour charger une Carte ou des Requêtes, rendez-vous dans &quot;Fichier&quot;, en haut à gauche de l'application." />
                           </children>
                        </TextFlow>
                     </content>
                  </ScrollPane>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
